#!/usr/bin/env node

var child_process = require("child_process");
var path = require("path");

var args = process.argv.slice(2);
var filepath = path.resolve(args.shift());

child_process.spawn("node", ["--harmony", "--harmony-tailcalls", "--use-strict", `${__dirname}/_simplebench.js`, filepath, ...args], { stdio : "inherit" });